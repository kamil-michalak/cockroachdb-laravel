{
    "name": "ylsideas/cockroachdb-laravel",
    "description": "This is my package cockroachdb-laravel",
    "keywords": [
        "ylsideas",
        "laravel",
        "cockroachdb-laravel"
    ],
    "homepage": "https://github.com/ylsideas/cockroachdb-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Fox",
            "email": "peter.fox@ylsideas.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "8.*"
    },
    "require-dev": {
        "doctrine/dbal": "^3.2",
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "YlsIdeas\\CockroachDb\\": "src",
            "YlsIdeas\\CockroachDb\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YlsIdeas\\CockroachDb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "YlsIdeas\\CockroachDb\\CockroachDbServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
